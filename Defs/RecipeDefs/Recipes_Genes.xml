<Defs>

	
	
	<RecipeDef>
		<defName>GR_BaseIncubator</defName>
		<label>manufacture empty incubator</label>
		<description>Manufacture an empty incubator using Steel and an organic growth compound.</description>
		<jobString>Manufacturing empty incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>800</workAmount>
		<recipeUsers>
				<li>GR_GeneticExtractionTable</li>	 
				<li>GR_AlphaSerumExtractionTable</li>	  
				
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
				
					<thingDefs>
						<li>Steel</li>
					
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
				
					<thingDefs>
						<li>GR_OrganicPulp</li>
						
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		
			<thingDefs>
				<li>Steel</li>
				<li>GR_OrganicPulp</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_EmptyIncubator>1</GR_EmptyIncubator>
		</products>
		
	</RecipeDef>
	
